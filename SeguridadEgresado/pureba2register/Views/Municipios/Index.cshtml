@model IEnumerable<pureba2register.Models.Municipio>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Departamento.NombreDepartamento)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NombreMunicipio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Departamento.NombreDepartamento)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NombreMunicipio)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MunicipioID }) |
